<%@ page contentType="text/html;charset=GBK" language="java" %>
<!DOCTYPE html>
<html>
	<head>
		<title>
			表达式语言――逻辑运算符
		</title>
	</head>
	<body>
		<table border="1" bgcolor="red">
			<tr>
				<td><b>表达式语言</b></td>
				<td><b>运算结果</b></td>
			</tr>
			<!-- 大于 -->
			<tr>
				<td>\${1 &gt; 2}</td>
				<td>${1 > 2}</td>
			</tr>
			<!-- 大于 -->
			<tr>
				<td>\${1 > 2}</td>
				<td>${1 gt 2}</td>
			</tr>
			<!-- 小于 -->
			<tr>
				<td>\${1 &lt; 2}</td>
				<td>${1 < 2}</td>
			</tr>
			<!-- 小于 -->
			<tr>
				<td>\${1 < 2}</td>
				<td>${1 lt 2}</td>
			</tr>
			<!-- 大于等于 -->
			<tr>
				<td>\${1 &ge; 2}</td>
				<td>${1 >= 2}</td>
			</tr>
			<!-- 大于等于 -->
			<tr>
				<td>\${1 >= 2}</td>
				<td>${1 ge 2}</td>
			</tr>
			<!-- 小于等于 -->
			<tr>
				<td>\${1 &le; 2}</td>
				<td>${1 <= 2}</td>
			</tr>
			<!-- 小于等于 -->
			<tr>
				<td>\${1 <= 2}</td>
				<td>${1 le 2}</td>
			</tr>
			<!-- 等于 -->
			<tr>
				<td>\${1 == 2}</td>
				<td>${1 == 2}</td>
			</tr>
			<!-- 等于 -->
			<tr>
				<td>\${1 eq 2}</td>
				<td>${1 eq 2}</td>
			</tr>
			<!-- 不等于 -->
			<tr>
				<td>\${1 != 2}</td>
				<td>${1 != 2}</td>
			</tr>
			<!-- 不等于 -->
			<tr>
				<td>\${1 ne 2}</td>
				<td>${1 ne 2}</td>
			</tr>
		</table>
	</body>
</html>

